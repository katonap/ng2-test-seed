{
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"outDir": "built/",
		"removeComments": false,
		"sourceMap": true,
		"target": "es5"
	},
	"exclude": [
		"node_modules"
	]
}